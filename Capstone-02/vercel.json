{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "regions": ["iad1"],
  "functions": {
    "index.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
